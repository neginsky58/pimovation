<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>Pimovation</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <meta name="MobileOptimized" content="320">
    <link href="/assets/font-awesome.min.css" rel="stylesheet" type="text/css"/>
    <link href="/assets/bootstrap.min.css" rel="stylesheet" type="text/css"/>
    <link href="/assets/uniform.default.css" rel="stylesheet" type="text/css"/>
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN PAGE LEVEL STYLES -->

    <!-- END PAGE LEVEL SCRIPTS -->
    <!-- BEGIN THEME STYLES -->
    <link href="/assets/style-metronic.css" rel="stylesheet" type="text/css"/>
    <link href="/assets/style.css" rel="stylesheet" type="text/css"/>
    <link href="/assets/style-responsive.css" rel="stylesheet" type="text/css"/>
    <link href="/assets/plugins.css" rel="stylesheet" type="text/css"/>
    <link href="/assets/default.css" rel="stylesheet" type="text/css" id="style_color"/>
    <link href="/assets/custom.css" rel="stylesheet" type="text/css"/>
    <!-- END THEME STYLES -->
    <link rel="shortcut icon" href="favicon.ico" />
    <script src="/assets/jquery-1.10.2.min.js" type="text/javascript"></script>
    <%= javascript_include_tag "jquery_ujs" %>
    <%= csrf_meta_tags %>
  </head>
  <body  class="page-header-fixed">
    <div class="header navbar navbar-inverse navbar-fixed-top">
      <!-- BEGIN TOP NAVIGATION BAR -->
      <div class="header-inner">
        <!-- BEGIN LOGO -->
        <a class="navbar-brand" href="index.html">
          <img src="/assets/grapple_logo.png" alt="logo" class="img-responsive" />
        </a>
        <!-- END LOGO -->
        <!-- BEGIN HORIZANTAL MENU -->
        <div class="hor-menu hidden-sm hidden-xs pull-right">
          <ul class="nav navbar-nav">
            <li class="<%= params[:action] == "dashboard" ? "active" : "" %>"><a href="<%=dashboard_mahheks_path%>">Home</a></li>
            <li class="<%= params[:action] == "profile" ? "active" : "" %>"><a href="<%=profile_mahheks_path%>">Edit Profile <span class="selected"></span></a></li>
            <li><a href="#">Help</a></li>
            <li><a href="#">Setting</a></li>
            <li>
              <%= link_to "Log Off", destroy_user_session_path, :method => :delete %>
            </li>
            <li>
              <span class="hor-menu-search-form-toggler">&nbsp;</span>
              <div class="search-form">
                <form class="form-search">
                  <div class="input-group">
                    <input type="text" placeholder="Search..." class="form-control">
                    <div class="input-group-btn">
                      <button type="button" class="btn"></button>
                    </div>
                  </div>
                </form>
              </div>
            </li>
          </ul>
        </div>
        <!-- END HORIZANTAL MENU -->
        <!-- BEGIN RESPONSIVE MENU TOGGLER -->
        <a href="javascript:;" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
          <img src="assets/img/menu-toggler.png" alt="" />
        </a>
        <!-- END RESPONSIVE MENU TOGGLER -->

      </div>
      <!-- END TOP NAVIGATION BAR -->
    </div>

    <%= yield %>
    <div class="footer">
      <div class="footer-inner">
        &copy; Grapple
      </div>
      <div class="footer-tools">
        <span class="go-top">
          <i class="icon-angle-up"></i>
        </span>
      </div>
    </div>


    <script src="/assets/jquery-migrate-1.2.1.min.js" type="text/javascript"></script>
    <script src="/assets/bootstrap.min.js" type="text/javascript"></script>
    <script src="/assets/twitter-bootstrap-hover-dropdown.min.js" type="text/javascript" ></script>
    <script src="/assets/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="/assets/jquery.blockui.min.js" type="text/javascript"></script>
    <script src="/assets/jquery.cookie.min.js" type="text/javascript"></script>
    <script src="/assets/jquery.uniform.min.js" type="text/javascript" ></script>
    <!-- END CORE PLUGINS -->
    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <script src="/assets/app.js" type="text/javascript"></script>


    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <!-- END PAGE LEVEL SCRIPTS -->
    <script>
      jQuery(document).ready(function() {
        App.init();
      
      });
    </script>
  </body>
</html>

